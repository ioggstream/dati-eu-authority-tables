<http://publications.europa.eu/resource/authority/concept-status/REVISED> a skos:Concept ;
    at:authority-code "REVISED" ;
    at:deprecated "false" ;
    at:op-code "REVISED" ;
    at:start.use "2018-02-20" ;
    atold:op-code "REVISED" ;
    dc:identifier "REVISED" ;
    skos:definition "The \"REVISED\" status indicates that a new record with updated labels for an existing code was created. As the code was already checked by the IMMC, there is no need to inform or submit it again for approval by the committee. The status will become \"CURRENT\" before publication. It is only used for internal management purposes and will not be published."@en ;
    skos:inScheme atold:concept-status ;
    skos:prefLabel "revised"@en ;
    skos:topConceptOf atold:concept-status .

