   <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/direct-award-justification/defse-excl"
                 at:deprecated="false">
      <skos:prefLabel xml:lang="bg">Особени изключвания за сектора на отбраната и особени изключвания за сектора на сигурността</skos:prefLabel>
      <skos:prefLabel xml:lang="cs">Zvláštní vyloučení specifická pro obranu a bezpečnost</skos:prefLabel>
      <skos:prefLabel xml:lang="da">Forsvars- og sikkerhedsspecifikke undtagelser</skos:prefLabel>
      <skos:prefLabel xml:lang="de">Ausnahmen bei Aufträgen, die Verteidigungs- und Sicherheitsaspekte beinhalten</skos:prefLabel>
      <skos:prefLabel xml:lang="el">Εξαιρέσεις για ειδικούς λόγους άμυνας και ασφάλειας</skos:prefLabel>
      <skos:prefLabel xml:lang="en">Defence-specific and security-specific exclusions</skos:prefLabel>
      <skos:prefLabel xml:lang="es">Exclusiones específicas por aspectos de defensa o seguridad</skos:prefLabel>
      <skos:prefLabel xml:lang="et">Kaitsealased ja julgeolekualased erandid</skos:prefLabel>
      <skos:prefLabel xml:lang="fi">Puolustus- ja turvallisuusaloja koskevat poikkeukset</skos:prefLabel>
      <skos:prefLabel xml:lang="fr">Exclusions en matière de défense et de sécurité</skos:prefLabel>
      <skos:prefLabel xml:lang="ga">Eisiaimh a bhaineann go sonrach leis an gcosaint agus leis an tslándáil</skos:prefLabel>
      <skos:prefLabel xml:lang="hr">Izuzeća specifična za obranu i sigurnost</skos:prefLabel>
      <skos:prefLabel xml:lang="hu">A védelemhez és a biztonsághoz kapcsolódó speciális kizárások</skos:prefLabel>
      <skos:prefLabel xml:lang="it">Esclusioni specifiche nei settori della difesa e della sicurezza</skos:prefLabel>
      <skos:prefLabel xml:lang="lt">Gynybos srities ir saugumo srities išimtys</skos:prefLabel>
      <skos:prefLabel xml:lang="lv">Ar aizsardzību un drošību saistīti izņēmumi</skos:prefLabel>
      <skos:prefLabel xml:lang="mt">Esklużjonijiet speċifiċi tad-difiża u tas-sigurtà</skos:prefLabel>
      <skos:prefLabel xml:lang="nl">Specifieke uitsluitingen voor defensie en veiligheid</skos:prefLabel>
      <skos:prefLabel xml:lang="pl">Wyłączenia szczególne w zakresie ochrony i bezpieczeństwa</skos:prefLabel>
      <skos:prefLabel xml:lang="pt">Exclusões exclusivas do setor da defesa e do setor da segurança</skos:prefLabel>
      <skos:prefLabel xml:lang="ro">Excluderi speciale din sectorul apărării și al securității</skos:prefLabel>
      <skos:prefLabel xml:lang="sk">Osobitné vylúčenia v oblasti obrany a bezpečnosti</skos:prefLabel>
      <skos:prefLabel xml:lang="sl">Izključitve pri naročilih, ki vključujejo obrambne ali varnostne vidike</skos:prefLabel>
      <skos:prefLabel xml:lang="sv">Försvarsspecifika och säkerhetsspecifika undantag</skos:prefLabel>
