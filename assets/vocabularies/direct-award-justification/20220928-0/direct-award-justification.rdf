      <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/direct-award-justification"/>
      <skos:scopeNote xml:lang="en">Directive 2009/81/EU, Article 28 (1d); Directive 2014/24/EU, Article 32 (2c); Directive 2014/25/EU, Article 50 (d)</skos:scopeNote>
   </skos:Concept>
