      <skos:prefLabel xml:lang="sk">Zákazky zadávané stranami, ktoré nie sú kupujúcimi</skos:prefLabel>
      <skos:prefLabel xml:lang="sl">Javna naročila, ki jih oddajo stranke, ki niso kupci</skos:prefLabel>
      <skos:prefLabel xml:lang="sv">Kontrakt som tilldelats av parter som inte är köpare</skos:prefLabel>
